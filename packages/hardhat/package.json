{
  "name": "@jimmychu0807/analytics-dapp-zama-hardhat",
  "description": "fhEVM hardhat template",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "author": {
    "name": "Jimmy Chu",
    "url": "https://jimmychu0807.hk"
  },
  "scripts": {
    "dev": "concurrently --names 'hardhat,mocked-server' --prefix '[{name}]' \"pnpm hardhat node --tags Analytic\" \"pnpm mock-server\"",
    "clean": "rimraf ./artifacts ./cache ./coverage ./types ./coverage.json && pnpm typechain",
    "compile": "hardhat compile",
    "mock-server": "tsx mockedServices/server.ts ",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm lint:sol && pnpm typecheck && pnpm lint:ts && pnpm prettier:check",
    "lint:fix": "pnpm lint:ts:fix && pnpm prettier:write",
    "lint:sol": "solhint --max-warnings 25 \"contracts/**/*.sol\"",
    "lint:ts": "eslint .",
    "lint:ts:fix": "eslint --fix .",
    "postinstall": "pnpm compile && pnpm typechain",
    "prettier:check": "prettier --check \"**/*.{js,json,md,sol,ts,yml}\"",
    "prettier:write": "prettier --write \"**/*.{js,json,md,sol,ts,yml}\"",
    "typechain": "hardhat typechain",
    "test": "hardhat test --network hardhat test/analytic/Analytic.test.ts",
    "test:reportGas": "REPORT_GAS=true hardhat test --network hardhat test/analytic/Analytic.test.ts",
    "test:all": "hardhat test --network hardhat",
    "test:all-reportGas": "REPORT_GAS=true hardhat test --network hardhat",
    "coverage": "hardhat coverage",
    "deploy-sepolia": "hardhat deploy --tags Analytic --network sepolia"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.12",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.4",
    "@types/cors": "^2.8.18",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.19.59",
    "@types/wait-on": "^5.3.4",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "bigint-buffer": "^1.1.5",
    "chai": "^4.3.7",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "catalog:",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^8.5.0",
    "ethers": "^6.8.0",
    "express": "^4.21.2",
    "extra-bigint": "^1.1.18",
    "fhevm": "^0.6.2",
    "fhevm-contracts": "^0.2.1",
    "fhevm-core-contracts": "^0.6.1",
    "fhevmjs": "catalog:",
    "fs-extra": "^10.1.0",
    "globals": "^15.9.0",
    "hardhat": "^2.22.8",
    "hardhat-deploy": "^0.12.4",
    "hardhat-gas-reporter": "^2.2.3",
    "hardhat-ignore-warnings": "^0.2.11",
    "hardhat-preprocessor": "^0.1.5",
    "lodash": "^4.17.21",
    "mocha": "^10.1.0",
    "prettier": "catalog:",
    "prettier-plugin-solidity": "^1.1.2",
    "rimraf": "^4.1.2",
    "sha3": "^2.1.4",
    "solady": "https://github.com/Vectorized/solady.git#v0.1.14",
    "solhint": "^3.4.0",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "0.8.12",
    "sqlite3": "^5.1.7",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.1",
    "tsx": "catalog:",
    "typechain": "^8.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "wait-on": "catalog:",
    "web3-validator": "^2.0.6"
  },
  "files": [
    "contracts"
  ],
  "keywords": [
    "blockchain",
    "ethers",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity",
    "template",
    "typescript",
    "typechain"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.2"
  },
